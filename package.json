{
  "name": "graphql-rs",
  "version": "0.0.5",
  "description": "",
  "main": "lib/index.js",
  "author": "Nigel Schuster <Neitsch@users.noreply.github.com>",
  "license": "MIT",
  "dependencies": {
    "neon-cli": "^0.3.3",
    "node-pre-gyp": "amilajack/node-pre-gyp#neon-compat",
    "node-pre-gyp-github": "^1.4.3"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build=false || neon build --release",
    "package": "node-pre-gyp package",
    "upload-binary": "node-pre-gyp package && node-pre-gyp-github publish",
    "build": "neon build",
    "test": "jest --projects lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neitsch/graphql-rs.git"
  },
  "files": [
    "native/src/*.rs",
    "native/build.rs",
    "native/Cargo.*",
    "lib"
  ],
  "binary": {
    "module_name": "index",
    "host": "https://github.com/Neitsch/graphql-rs/releases/download/",
    "remote_path": "{version}",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz",
    "module_path": "./native",
    "pkg_path": "."
  },
  "devDependencies": {
    "graphql": "^14.3.0",
    "jest": "^27.0.1"
  }
}
